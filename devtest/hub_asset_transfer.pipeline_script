node {
	stage('Asset transfer') {
		echo "Moving files to ${params.Prison}"
		sh "ssh -R 50000:${params.Prison}.mycloudgateway.co.uk:22 digital-hub-stage.hmpps.dsd.io 'rsync -n -riP --ignore-existing -e "ssh -p50000 -o PasswordAuthentication=yes -oStrictHostKeyChecking=no" /content/moj_dhub_prod001_app/usr/share/nginx/html/moj_be/sites/default/files/ root@localhost:/content/moj_dhub_prod001_app/usr/share/nginx/html/moj_be/sites/default/files/'"
	}
}
