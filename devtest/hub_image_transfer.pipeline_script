node {
	withCredentials([sshUserPrivateKey(credentialsId: "29c48f15-66b3-4a8c-b391-60782af72251", keyFileVariable: 'keyfile')]) {
		stage('Asset transfer') {
			echo "Moving ${params.Component} to ${params.Prison}"
			
			sh """
				docker save ${params.Component} | ssh -i "${keyfile}" -oStrictHostKeyChecking=no ${params.Prison}.mycloudgateway.co.uk docker load'   
			"""		
			
			echo "Moved ${params.Component} to ${params.Prison}"
		}
	}
}